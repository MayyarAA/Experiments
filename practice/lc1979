class Solution {
    public int findGCD(int[] nums) {
        Arrays.sort(nums);
        int res =1;
        int maxDiv=1;
        while(res<=nums[0]){
            if(nums[0]%res==0 && nums[nums.length-1]%res==0 ){
                maxDiv=res;
            }
            res++;
        }
        return maxDiv;
    }
}
