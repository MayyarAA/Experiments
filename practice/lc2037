class Solution {
    public int minMovesToSeat(int[] seats, int[] students) {
        int res=0;
        /**
        seats[i] = post of ith seat
        students[j] = post of jth student
        min moves
        **/
        Arrays.sort(seats);Arrays.sort(students);
        for(int i=0;i<seats.length;i++){
            res+= Math.abs(seats[i]-students[i]);
        }
        return res;
    }
}
